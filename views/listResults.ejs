    </header>
    <main>
     
      <table  class="center"  border  = "1">
 <tr>
   <th>Name</th>
   <th>Breakfast</th>
   <th>Lunch</th>
   <th>Dinner</th>
   <th>Activities</th>
   <th>Shift Summary</th>
 </tr>
    <% for(let i = 0; i <success.length; i++) { %>
        <tr>
            <td><%= success[i].name %></td>
            <td><%= success[i].breakfast %></td>
          <td><%= success[i].lunch %></td>
          <td><%= success[i].dinner %></td>
          <td><%= success[i].activities %></td>
          <td><%= success[i].shift_summary %></td>
        </tr>
    <% } %>
</table>


    </main>
